    <%- include("header") -%>

    <div class="box" id="heading">
        <h1><%= tituloLista %></h1>
    </div>
    <div class="box">
        <% for (var i = 0; i < novoItem.length; i++){%>
        <div class="item" >
            <input type="checkbox">
            <p> <%= novoItem[i] %> </p>

        </div>
        <% } %>

        <form action="/" method="post">
            <input type="text" name="addlista" id="addlista" placeholder="Novo Item" autocomplete="off">
            <button type="submit" name="button" value="<%= tituloLista %>">+</button>
        </form> 
    </div>

    <%- include("footer") -%>